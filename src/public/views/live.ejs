<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script>
        document.addEventListener("DOMContentLoaded", () => {
            const streamName = "<%= streamName %>";
            const streamNames = streamName.split(',');

            const ul = document.querySelector("ul");

            streamNames.forEach((name) => {
                const li = document.createElement("li");
                const a = document.createElement("a");
                a.rel = "stylesheet";
                a.href = `https://dolbyio-streaming-272abf7507d6.herokuapp.com/viewer/${name}`;
                a.textContent = `https://dolbyio-streaming-272abf7507d6.herokuapp.com/viewer/${name}`;
                li.appendChild(a);
                ul.appendChild(li);
            });
        });
    </script>
</head>
<body>
    <ul>
        <!-- Os links serÃ£o adicionados aqui pelo JavaScript -->
    </ul>
</body>
</html>